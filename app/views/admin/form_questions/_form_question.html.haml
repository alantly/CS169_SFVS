%div.row
  %div.form_q{"data-id" => form_question.id, "q-order" => form_question.order}
    %div.span5
      %li= form_question.question
    %div.span2
      %li= form_question.question_type
    %div.span4
      %li= form_question.created_at
    %div.span3
      %li= form_question.updated_at
    %div.form_q_button.span2
      %li= link_to "Edit", edit_admin_form_form_question_path(:form_id => @form.id, :id=> form_question.id), {:class => "btn"}
      %li= link_to "Delete", admin_form_form_question_path(:form_id => @form.id, :id=> form_question.id), {:class => "btn btn-danger", method: :delete, data: { confirm: "Are you sure?"}}
  